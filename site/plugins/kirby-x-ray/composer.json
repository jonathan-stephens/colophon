{
  "name": "beebmx/kirby-x-ray",
  "description": "X-Ray provides a comprehensive overview of the content present on your site and individual pages, allowing you to inspect and evaluate them at a glance.",
  "license": "MIT",
  "type": "kirby-plugin",
  "version": "1.0.5",
  "homepage": "https://github.com/beebmx/kirby-x-ray",
  "keywords": [
    "xray",
    "x-ray",
    "stats",
    "kirby",
    "kirby-plugin",
    "kirby5"
  ],
  "authors": [
    {
      "name": "Fernando Gutierrez",
      "homepage": "https://beeb.mx"
    }
  ],
  "require": {
    "getkirby/composer-installer": "^1.2"
  },
  "require-dev": {
    "getkirby/cms": "^5.0",
    "laravel/pint": "^1.0",
    "pestphp/pest": "^3.8",
    "spatie/ray": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "autoload": {
    "psr-4": {
      "Beebmx\\KirbyXRay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "extra": {
    "kirby-cms-path": false,
    "kirby-plugin-path": "tests/Fixtures/site/plugins"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
